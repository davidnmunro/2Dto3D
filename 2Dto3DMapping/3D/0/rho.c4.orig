FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho.c4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1601;

boundaryField
{
    ground
    {
        type            zeroGradient;
    }

    midPlane1
    {
        type            symmetry;
    }
    midPlane2
    {
        type            symmetry;
    }

    outlet
    {
        type            zeroGradient;
    }
}

